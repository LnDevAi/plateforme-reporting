parameters:
    level: 1
    paths:
        - app/
    ignoreErrors:
        - '#Call to an undefined method Illuminate\\.*#'
        - '#Class .* not found#'
        - '#should return .* but returns .*#'
    excludePaths:
        - app/Console/Kernel.php
        - app/Http/Kernel.php
        - app/Exceptions/Handler.php
    tmpDir: storage/tmp/phpstan