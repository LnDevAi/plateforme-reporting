# Sanctum Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,127.0.0.1:3000,::1,localhost:3000

# AI Writing Assistant Configuration
# Clé API OpenAI pour l'assistant IA de rédaction
OPENAI_API_KEY=your_openai_api_key_here

# Configuration OpenAI (optionnel)
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2000
OPENAI_TIMEOUT=60

# Fournisseurs IA alternatifs (optionnel)
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_ENABLED=false

GOOGLE_AI_API_KEY=
GOOGLE_AI_MODEL=gemini-pro
GOOGLE_AI_ENABLED=false

# Configuration des services IA (Assistant conversationnel)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_openai_organization_id
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=claude-3-sonnet-20240229
CLAUDE_MAX_TOKENS=2000
CLAUDE_API_VERSION=2023-06-01

AI_DEFAULT_PROVIDER=openai
AI_FALLBACK_ENABLED=true
AI_CACHE_RESPONSES=false
AI_RATE_LIMIT_PER_MINUTE=60
AI_CONVERSATION_TIMEOUT_HOURS=24