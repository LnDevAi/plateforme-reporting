APP_NAME="Plateforme EPE"
APP_ENV=production
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_DEBUG=false
APP_URL=https://your-domain.com

LOG_CHANNEL=cloudwatch
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# Base de données AWS RDS
DB_CONNECTION=mysql
DB_HOST=${DB_HOST}
DB_PORT=${DB_PORT}
DB_DATABASE=${DB_DATABASE}
DB_USERNAME=${DB_USERNAME}
DB_PASSWORD=${DB_PASSWORD}

# Cache et sessions AWS
BROADCAST_DRIVER=redis
CACHE_DRIVER=redis
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=sqs
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# Redis AWS ElastiCache
REDIS_HOST=${REDIS_HOST}
REDIS_PASSWORD=${REDIS_PASSWORD}
REDIS_PORT=6379

# Mail AWS SES
MAIL_MAILER=ses
MAIL_HOST=email-smtp.us-east-1.amazonaws.com
MAIL_PORT=587
MAIL_USERNAME=${SES_USERNAME}
MAIL_PASSWORD=${SES_PASSWORD}
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="noreply@your-domain.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Services
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=${S3_BUCKET}
AWS_USE_PATH_STYLE_ENDPOINT=false

# Queue AWS SQS
SQS_PREFIX=${SQS_PREFIX}
SQS_QUEUE=${SQS_QUEUE}

# Services IA Production
OPENAI_API_KEY=${OPENAI_API_KEY}
CLAUDE_API_KEY=${CLAUDE_API_KEY}
GOOGLE_AI_API_KEY=${GOOGLE_AI_API_KEY}
AI_DEFAULT_PROVIDER=openai
AI_FALLBACK_ENABLED=true
AI_CACHE_RESPONSES=true
AI_RATE_LIMIT_PER_MINUTE=100

# Services de paiement
STRIPE_KEY=${STRIPE_KEY}
STRIPE_SECRET=${STRIPE_SECRET}
STRIPE_WEBHOOK_SECRET=${STRIPE_WEBHOOK_SECRET}

# Monitoring & Observabilité
SENTRY_LARAVEL_DSN=${SENTRY_DSN}

# Optimisation Production
CACHE_DRIVER=redis
SESSION_DRIVER=redis
QUEUE_CONNECTION=sqs
BROADCAST_DRIVER=redis

# Sécurité
SESSION_SECURE_COOKIE=true
SANCTUM_STATEFUL_DOMAINS=your-domain.com,www.your-domain.com