{
	"version": 1,
	"defaultProject": "reporting-frontend",
	"projects": {
		"reporting-frontend": {
			"projectType": "application",
			"root": "",
			"sourceRoot": "src",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/reporting-frontend",
						"index": "src/index.html",
						"main": "src/main.ts",
						"tsConfig": "tsconfig.app.json",
						"polyfills": [],
						"assets": ["src/favicon.ico", "src/assets"],
						"styles": [],
						"scripts": []
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "reporting-frontend:build"
					}
				}
			}
		}
	}
}